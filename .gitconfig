[user]
	name = Alex Tebbs
	email = alex@alextebbs.com
[core]
	autocrlf = false
	safecrlf = true
	excludesfile = /Users/brodiac/.gitignore
[alias]
	co = commit
	st = status
	br = branch
	ck = checkout
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	pom = push origin master
	switch = !legit switch \"$@\"
	branches = !legit branches
	sprout = !legit sprout \"$@\"
	unpublish = !legit unpublish \"$@\"
	harvest = !legit harvest \"$@\"
	sync = !legit sync \"$@\"
	publish = !legit publish \"$@\"
	graft = !legit graft \"$@\"
